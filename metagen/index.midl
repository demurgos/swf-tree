struct Movie {
  header: Header;
  tags: Vector<Tag>;
}

struct Header {
  swf_version: Uint8;
  frame_size: Rect;
}

struct Rect {
  x_min: Sint32;
  x_max: Sint32;
  y_min: Sint32;
  y_max: Sint32;
}

sum Tag {
  ShowFrame: tags.ShowFrame;
  Telemetry: tags.Telemetry;
}

mod tags {
  struct ShowFrame {}

  struct Telemetry {
    password: Bytes;
  }

  struct Color {
    space: ColorSpace;
  }
}

enum ColorSpace {
  SRgb;
  LinearRgb;
}
