{
  "name": "swf-tree",
  "version": "0.0.11",
  "homepage": "https://github.com/open-flash/swf-tree",
  "description": "Abstract Syntax Tree for SWF",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:open-flash/swf-tree.git"
  },
  "keywords": [
    "swf",
    "ast"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "gulp all:tsconfig.json && gulp :tslint.json && gulp dist",
    "test": "gulp :lint && gulp test",
    "prestart": "gulp example:build",
    "start": "node build/example/example/main.js",
    "prepublishOnly": "npm test",
    "lint": "gulp :lint:fix"
  },
  "dependencies": {
    "incident": "^3.1.1",
    "kryo": "^0.7.0-beta.2",
    "semantic-types": "^0.1.1",
    "ts-tagged": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.3",
    "chai": "^4.1.2",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "minimist": "^1.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "turbo-gulp": "^0.16.2",
    "typescript": "^2.7.1"
  },
  "nyc": {
    "include": [
      "build/test/lib/**/*.js",
      "build/test/lib/**/*.mjs"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "extension": [
      ".mjs"
    ]
  },
  "@std/esm": {
    "esm": "cjs"
  }
}
